@model Model.WebModel.User

@{
    ViewData["Title"] = "ViewProfile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>View Profile: @Model.UserName</h1>

<div>
    <hr />
    <dl class="row">        
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.UserName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ProfileText)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ProfileText)
        </dd>
    </dl>
</div>
<div class="row">
    @await Html.PartialAsync("Partial/ReviewList", Model.ReviewList)
</div>
